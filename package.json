{
  "name": "soundcloud-redux",
  "version": "0.4.0",
  "description": "SoundCloud API client with React, Redux, and Redux Saga",
  "homepage": "https://github.com/r-park/soundcloud-redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/soundcloud-redux.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean:coverage": "del-cli coverage",
    "clean:target": "del-cli target",
    "heroku-postbuild": "npm run build",
    "lint": "eslint -c .eslintrc.yml *.js src server",
    "prebuild": "npm run clean:target",
    "pretest": "run-s clean:coverage lint",
    "pretest:server": "npm run build",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm run server:dev",
    "test": "run-s test:client test:server",
    "test:client": "cross-env NODE_ENV=test karma start --single-run",
    "test:server": "ava server/spec.js --verbose",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "compression": "~1.6.2",
    "express": "~4.14.0",
    "helmet": "~2.2.0",
    "serve-favicon": "~2.3.0",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "~6.4.1",
    "ava": "~0.16.0",
    "babel-core": "~6.14.0",
    "babel-eslint": "~6.1.2",
    "babel-loader": "~6.2.5",
    "babel-plugin-istanbul": "~2.0.1",
    "babel-plugin-transform-function-bind": "~6.8.0",
    "babel-polyfill": "~6.13.0",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-react-optimize": "~1.0.1",
    "babel-preset-stage-1": "~6.13.0",
    "classnames": "~2.2.5",
    "conventional-changelog-cli": "~1.2.0",
    "cross-env": "~3.0.0",
    "css-loader": "~0.25.0",
    "del-cli": "~0.2.0",
    "enzyme": "~2.4.1",
    "eslint": "~3.6.0",
    "eslint-plugin-react": "~6.3.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "html-webpack-plugin": "~2.22.0",
    "immutable": "~3.8.1",
    "jasmine-core": "~2.5.2",
    "json-loader": "~0.5.4",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-mocha-reporter": "~2.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.0",
    "minx": "r-park/minx.git",
    "morgan": "~1.7.0",
    "node-sass": "~3.10.0",
    "nodemon": "~1.10.2",
    "npm-run-all": "~3.1.0",
    "postcss-loader": "~0.13.0",
    "raw-loader": "~0.5.1",
    "react": "~15.3.2",
    "react-addons-test-utils": "~15.3.2",
    "react-dom": "~15.3.2",
    "react-hot-loader": "3.0.0-beta.5",
    "react-redux": "~4.4.5",
    "react-router": "~2.8.1",
    "react-router-scroll": "~0.3.2",
    "redux": "~3.6.0",
    "redux-saga": "~0.11.1",
    "reselect": "~2.5.4",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "~4.0.2",
    "sinon": "1.17.6",
    "style-loader": "~0.13.1",
    "superagent": "~2.3.0",
    "supertest": "~2.0.0",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.4",
    "webpack-md5-hash": "~0.0.5"
  }
}
