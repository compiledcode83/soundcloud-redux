{
  "name": "soundcloud-redux",
  "version": "0.3.0",
  "description": "soundcloud-redux",
  "homepage": "https://github.com/r-park/soundcloud-redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/soundcloud-redux.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "run-s clean:coverage clean:target",
    "clean:coverage": "del-cli coverage",
    "clean:target": "del-cli target",
    "heroku-postbuild": "npm run build",
    "lint": "eslint -c .eslintrc.yml *.js src server",
    "prebuild": "npm run clean:target",
    "pretest": "run-s clean:coverage lint",
    "pretest:server": "npm run build",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:dev",
    "test": "run-s test:client test:server",
    "test:client": "cross-env NODE_ENV=test karma start --single-run --coverage",
    "test:server": "ava server/spec.js --verbose",
    "test:watch": "cross-env NODE_ENV=test karma start --reporters dots",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "compression": "~1.6.2",
    "express": "~4.14.0",
    "helmet": "~2.1.1",
    "serve-favicon": "~2.3.0",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.7",
    "ava": "~0.15.2",
    "babel-core": "~6.11.4",
    "babel-eslint": "~6.1.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-function-bind": "~6.8.0",
    "babel-polyfill": "~6.9.1",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-react-optimize": "~1.0.1",
    "babel-preset-stage-1": "~6.5.0",
    "classnames": "~2.2.5",
    "conventional-changelog-cli": "~1.2.0",
    "cross-env": "~2.0.0",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "enzyme": "~2.4.1",
    "eslint": "~3.2.0",
    "eslint-plugin-react": "6.0.0-rc.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-webpack-plugin": "~2.22.0",
    "immutable": "~3.8.1",
    "isparta-loader": "~2.0.0",
    "jasmine-core": "~2.4.1",
    "json-loader": "~0.5.4",
    "karma": "~1.1.2",
    "karma-chrome-launcher": "~1.0.1",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-mocha-reporter": "~2.1.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.7.0",
    "minx": "r-park/minx.git",
    "morgan": "~1.7.0",
    "node-sass": "~3.8.0",
    "nodemon": "~1.10.0",
    "npm-run-all": "~2.3.0",
    "postcss-loader": "~0.9.1",
    "raw-loader": "~0.5.1",
    "react": "~15.3.0",
    "react-addons-test-utils": "~15.3.0",
    "react-dom": "~15.3.0",
    "react-hot-loader": "3.0.0-beta.2",
    "react-redux": "~4.4.5",
    "react-router": "~2.6.1",
    "react-router-scroll": "~0.2.1",
    "redux": "~3.5.2",
    "redux-saga": "~0.11.0",
    "reselect": "~2.5.3",
    "rxjs": "5.0.0-beta.10",
    "sass-loader": "~4.0.0",
    "sinon": "1.17.5",
    "style-loader": "~0.13.1",
    "superagent": "~2.1.0",
    "supertest": "~2.0.0",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1",
    "webpack-md5-hash": "~0.0.5",
    "yargs": "~4.8.1"
  }
}
